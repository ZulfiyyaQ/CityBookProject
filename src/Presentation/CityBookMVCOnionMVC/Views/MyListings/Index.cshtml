@model PaginationVM<ItemPlaceVM>
<div id="wrapper">
    <!--content -->
    <div class="content">
        <!--section -->
        <section id="sec1">
            <!-- container -->
            <div class="container">
                <!-- profile-edit-wrap -->
                <div class="profile-edit-wrap">
                    <div class="profile-edit-page-header">
                        <h2>Messages </h2>
                        <div class="breadcrumbs"><a href="#">Home</a><a href="#">Dasboard</a><span>Messages</span></div>
                    </div>
                    <div class="row">
                        <div class="col-md-3">
                            <div class="fixed-bar fl-wrap">
                                <div class="user-profile-menu-wrap fl-wrap">
                                    <!-- user-profile-menu-->
                                    <div class="user-profile-menu">
                                        <h3>Main</h3>
                                        <ul>

                                            <li><a asp-action="index" asp-controller="myaccount"><i class="fa fa-user-o"></i> Edit profile</a></li>
                                            <li><a asp-action="changepassword" asp-controller="account" class="user-profile-act"><i class="fa fa-unlock-alt"></i>Change Password</a></li>
                                            <li><a asp-action="forgotpassword" asp-controller="account" class="user-profile-act"><i class="fa fa-unlock-alt"></i>Forgot Password</a></li>
                                        </ul>
                                    </div>
                                    <!-- user-profile-menu end-->
                                    <!-- user-profile-menu-->
                                    @if (User.IsInRole(UserRole.BiznesOwner.ToString()))
                                    {
                                        <div class="user-profile-menu">
                                            <h3>Listings</h3>

                                            <ul>
                                                <li><a asp-action="index" asp-controller="mylistings"><i class="fa fa-th-list"></i> My listigs  </a></li>
                                                <li><a href="dashboard-bookings.html"> <i class="fa fa-calendar-check-o"></i> Bookings <span>2</span></a></li>
                                                <li><a href="dashboard-review.html"><i class="fa fa-comments-o"></i> Reviews </a></li>
                                                <li><a asp-action="create" asp-controller="addlisting"><i class="fa fa-plus-square-o"></i> Add New Place</a></li>
                                                <li><a asp-action="create" asp-controller="addblog"><i class="fa fa-plus-square-o"></i> Add New Blog</a></li>
                                            </ul>
                                        </div>
                                    }

                                    <!-- user-profile-menu end-->
                                    <a asp-action="logout" asp-controller="account" class="log-out-btn">Log Out</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-9">
                            <div class="dashboard-list-box fl-wrap">
                                <div class="dashboard-header fl-wrap">
                                    <h3>Indox</h3>
                                </div>
                                @foreach (var item in Model.Items)
                                {
                                    <div class="dashboard-list">
                                        <div class="dashboard-message">
                                            <span class="new-dashboard-item">New</span>
                                            <div class="dashboard-listing-table-image">
                                                <a href="listing-single.html"><img src="~/images/@item.PlaceImages.FirstOrDefault()?.ImageUrl" alt="@item.Name"></a>
                                            </div>
                                            <div class="dashboard-listing-table-text">
                                                <h4><a asp-action="detail" asp-controller="listing" asp-route-id="@item.Id">@item.Name</a></h4>
                                                <span class="dashboard-listing-table-address"><i class="fa fa-map-marker"></i><a href="#">@item.Address</a></span>
                                             @*    <div class="listing-rating card-popup-rainingvis fl-wrap" data-starrating2="5">
                                                    <span>(@item.Review.Count())</span>
                                                </div> *@
                                                <ul class="dashboard-listing-table-opt  fl-wrap">
                                                    <li><a asp-action="update" asp-controller="mylisting">Edit <i class="fa fa-pencil-square-o"></i></a></li>
                                                    <li><a asp-action="delete" asp-controller="mylisting" class="del-btn">Delete <i class="fa fa-trash-o"></i></a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                }
                                
                              
                            </div>
                            <!-- pagination-->
                            <div class="pagination">
                                <a href="#" class="prevposts-link"><i class="fa fa-caret-left"></i></a>
                                <a href="#">1</a>
                                <a href="#" class="current-page">2</a>
                                <a href="#">3</a>
                                <a href="#">4</a>
                                <a href="#" class="nextposts-link"><i class="fa fa-caret-right"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
                <!--profile-edit-wrap end -->
            </div>
            <!--container end -->
        </section>
        <!-- section end -->
        <div class="limit-box fl-wrap"></div>
        <!--section -->
        
        <!-- section end -->
    </div>
</div>